{
  "name": "ng-x-rocket",
  "version": "1.0.0",
  "private": true,
  "watch": {
    "dev-build": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html,css"
    }
  },
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod && workbox generate:sw",
    "watch": "npm-watch dev-build",
    "dev-build": "ng build && workbox generate:sw",
    "dev": "npm run ng build && concurrently -r --kill-others 'node live-server' 'npm run watch'",
    "start": "ng serve --base-href=/app/ && workbox-cli generate:sw",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "4.4.6",
    "@angular/common": "4.4.6",
    "@angular/compiler": "4.4.6",
    "@angular/core": "4.4.6",
    "@angular/forms": "4.4.6",
    "@angular/http": "4.4.6",
    "@angular/platform-browser": "4.4.6",
    "@angular/platform-browser-dynamic": "4.4.6",
    "@angular/router": "4.4.6",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-beta.6",
    "@ngx-translate/core": "8.0.0",
    "@thisissoon/angular-inviewport": "1.3.1",
    "bootstrap": "4.0.0-beta",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "jquery": "3.2.1",
    "live-server": "1.2.0",
    "lodash": "4.17.4",
    "node-sass": "4.9.0",
    "npm-watch": "0.3.0",
    "rxjs": "5.5.2",
    "zone.js": "0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "4.4.7",
    "@angular/language-service": "4.4.7",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@ngx-rocket/scripts": "1.1.0",
    "@types/jasmine": "2.8.16",
    "@types/jasminewd2": "2.0.6",
    "@types/lodash": "4.14.121",
    "@types/node": "8.10.42",
    "codelyzer": "3.2.2",
    "hads": "1.7.2",
    "htmlhint": "0.9.13",
    "https-proxy-agent": "2.2.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "4.1.0",
    "karma": "~1.7.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.3",
    "karma-jasmine": "1.1.2",
    "karma-junit-reporter": "1.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "phantomjs-prebuilt": "2.1.16",
    "protractor": "~5.1.2",
    "stylelint": "~8.1.0",
    "stylelint-config-recommended-scss": "~2.0.0",
    "stylelint-config-standard": "~17.0.0",
    "stylelint-scss": "~2.1.0",
    "ts-node": "3.2.0",
    "tslint": "~5.7.0",
    "typescript": "2.3.4",
    "workbox-cli": "2.1.3"
  }
}
